    },
    "first-instruction": {
      "value": "The Pedal Up Trigger value cannot be higher than the Pedal Down Trigger value.",
      "type": "text"
    },
    "instruction-header": {
      "value": "This option allows you to set the switching point of the pedal, i.e. you determine at which position of the mechanical pedal the electronic pedal reacts.",
      "type": "text"
    },
    "minimum-gap-hint": {
      "value": "Note: The two values must have a minimum distance of {{ minimumGap }}.",
      "type": "text"
    },
    "second-instruction": {
      "value": "The lower the value, the “higher” (in the sense of a higher position) is the corresponding trigger point.",
      "type": "text"
    },
    "third-instruction": {
      "value": "The adjustment of the trigger points is recommended after each pedal calibration.",
      "type": "text"
    }
  },
